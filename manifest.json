{
  "name": "New Tabs To The Right",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension will force all the new tabs to open to the right (end).",
  "homepage_url": "http://www.fluxbytes.com",
  "icons": {
    "16": "icons/18.png",
    "48": "icons/46.png",
    "128":"icons/128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs"
  ]
}